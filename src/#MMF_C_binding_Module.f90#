MODULE OPENMMF_C_WRAPPER

  USE ISO_C_BINDING, ONLY :: C_DOUBLE, C_INT
  USE ATOMIC_PROPERTIES
  USE TYPES

  IMPLICIT NONE
  CONTAINS
    SUBROUTINE SET_ATOMIC_PARAMETERS_C(ATOMICSPECIE,MANIFOLD,JTOTAL_,ID,INFO) BIND(C,NAME='set_atomic_parameters')
      IMPLICIT NONE
!      CHARACTER*(*),OPTIONAL, INTENT(IN) :: ATOMICSPECIE
!      CHARACTER*(*),OPTIONAL, INTENT(IN) :: MANIFOLD  !
      POINTER(C_CHAR_P),OPTIONAL, INTENT(IN) :: ATOMICSPECIE
      POINTER(C_CHAR_P),OPTIONAL, INTENT(IN) :: MANIFOLD  !
      REAL(C_DOUBLE),  OPTIONAL, INTENT(IN) :: JTOTAL_
      TYPE(ATOM),INTENT(OUT) :: ID
      INTEGER(C_INT), INTENT(INOUT) :: INFO      
      CALL SET_ATOMIC_PARAMETERS(ATOMICSPECIE,MANIFOLD,JTOTAL_,ID,INFO)
    END SUBROUTINE SET_ATOMIC_PARAMETERS_C

END MODULE OPENMMF_C_WRAPPER
